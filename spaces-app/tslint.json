{
    "rules": {
        "class-name": true,
        "indent": [
            true,
            "spaces"
        ],
        "no-duplicate-variable": true,
        "no-eval": true,
        "no-internal-module": true,
        "no-unsafe-finally": true,
        "no-var-keyword": true,
        "triple-equals": [
            true,
            "allow-null-check"
        ],  
        "variable-name": [
            true,
            "ban-keywords"
        ],
        "no-unused-expression": true,
        "no-unused-variable": [true, {"ignore-pattern": "^_"}]
    }
}
